<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>INKSCAPE V-3</Name>
  <Description>Inkscape is a Free and open source vector graphics editor for GNU/Linux, Windows and MacOS X. &#13;
[br][br][b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]&#13;
[b]/config[/b] : Folder where Inkscape configuration is stored[br]&#13;
[b]8080[/b] : xRDP web-interface.[br]
[b]3389[/b] : xRDP client interface</Description>
  <Registry>accetto/ubuntu-vnc-xfce-inkscape-g3:vnc-novnc-firefox-plus</Registry>
  <Repository>accetto/ubuntu-vnc-xfce-inkscape-g3:vnc-novnc-firefox-plus</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>35901</HostPort>
        <ContainerPort>5901</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>36901</HostPort>
        <ContainerPort>6901</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/inkscape-g3</HostDir>
      <ContainerDir>/home/headless/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/media/images</HostDir>
      <ContainerDir>/home/headless/Desktop/Images/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:36901]</WebUI>
  <Banner>https://media.inkscape.org/static/images/inkscape-logo.svg</Banner>
  <Icon>https://media.inkscape.org/static/images/inkscape-logo.svg</Icon>
  <ExtraParams></ExtraParams>
  <Overview>Inkscape is a Free and open source vector graphics editor for GNU/Linux, Windows and MacOS X.</Overview>
  <Beta>Truee</Beta>
  <Category>Productivity:</Category>
  <Date>2019-10-29</Date> 
  <Support></Support>
</Container>
