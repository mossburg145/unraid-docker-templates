<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>DRAWIO V-3</Name>
  <Description>Draw IO &#13;
[br][br][b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]&#13;
[b]/config[/b] : Folder where Inkscape configuration is stored[br]&#13;
[b]8080[/b] : xRDP web-interface.[br]
[b]3389[/b] : xRDP client interface</Description>
  <Registry>accetto/ubuntu-vnc-xfce-drawio-g3:vnc-novnc-firefox-plus</Registry>
  <Repository>accetto/ubuntu-vnc-xfce-drawio-g3:vnc-novnc-firefox-plus</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>45901</HostPort>
        <ContainerPort>5901</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>46901</HostPort>
        <ContainerPort>6901</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/drawio-g3</HostDir>
      <ContainerDir>/home/headless/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/media/images</HostDir>
      <ContainerDir>/home/headless/Desktop/Images/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:46901]</WebUI>
  <Banner>https://drawio-app.com/wp-content/uploads/2020/11/drawio_logo_RGB_dark_mini_199x50px.png</Banner>
  <Icon>https://avatars.githubusercontent.com/u/921200?v=4</Icon>
  <ExtraParams></ExtraParams>
  <Overview>Inkscape is a Free and open source vector graphics editor for GNU/Linux, Windows and MacOS X.</Overview>
  <Beta>Truee</Beta>
  <Category>Productivity:</Category>
  <Date>2019-10-29</Date> 
  <Support></Support>
</Container>
